{
  "$defs": {
    "Analysis": {
      "additionalProperties": false,
      "properties": {
        "summary": {
          "$ref": "#/$defs/SummaryAnalysis",
          "default": {
            "include": false
          }
        },
        "cytoscape": {
          "$ref": "#/$defs/CytoscapeAnalysis",
          "default": {
            "include": false
          }
        },
        "ml": {
          "$ref": "#/$defs/MlAnalysis",
          "default": {
            "include": false,
            "aggregate_per_algorithm": false,
            "components": 2,
            "labels": true,
            "linkage": "ward",
            "metric": "euclidean"
          }
        },
        "evaluation": {
          "$ref": "#/$defs/EvaluationAnalysis",
          "default": {
            "include": false,
            "aggregate_per_algorithm": false
          }
        }
      },
      "title": "Analysis",
      "type": "object"
    },
    "ContainerFramework": {
      "enum": [
